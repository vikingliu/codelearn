def catalan_stack_out(nums):
    n = len(nums)
    dp = [0] * (n + 1)
    dp[0] = [[]]
     # dp[i] = ∑dp[k]dp[i-k-1],  0≤k<i
    for i in range(1, n + 1):
        dp[i] = []
        for j in range(0, i):
            left = dp[j]
            right = dp[i - j - 1]
            for l in left:
                for r in right:
                    dp[i].append(l + [nums[i - 1]] + r)
    return dp[-1]

catalan_stack_out([1,2,3,4])






